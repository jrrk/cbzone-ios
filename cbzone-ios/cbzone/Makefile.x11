install:: cbzone.help
	$(INSTALL) -c $(INSTDATFLAGS) cbzone.help $(TANKDIR)

install:: cbzone.motd
	$(INSTALL) -c $(INSTDATFLAGS) cbzone.motd $(TANKDIR)

tar: cbzone.tar.Z

